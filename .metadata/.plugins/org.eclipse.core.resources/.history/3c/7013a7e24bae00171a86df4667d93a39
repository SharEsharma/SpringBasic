<script type="text/javascript">
//ComapnyInfo ajax function and validation of fields
//created by Supriya & Mitali
function ComapnyInfo(){
	 var a=0;
	 if($("#imageid").val()==0){
	    	alert("imageid should not be empty");
	    }
	 else if($("#headofficeaddress").val()==0){
	    	alert("headofficeaddress should not be empty");
	    }
	 else if($("#head_country").val()==""){
	    	alert("head_country should not be empty");
	    }
	 else if($("#head_state").val()==""){
	    	alert("head_state should not be empty");
	    }
	 else if($("#head_city").val()==""){
	    	alert("head_city should not be empty");
	    }
	 
	 else if($("#check").is(":checked")==false){
		 if($("#currentaddress").val()==""){
		    	alert("currentaddress should not be empty");
		    }
		 else if($("#branch_country").val()==""){
		    	alert("branch_country should not be empty");
		    }
		 else  if($("#branch_state").val()==""){
		    	alert("branch_state should not be empty");
		    }
		 else  if($("#branch_city").val()==""){
		    	alert("branch_city should not be empty");
		    }
		 else{
			 a=1;
		 }
	 }
	 else if($("#check").is(":checked")==true){
		 a=1;
	 }
	  if(a==1){
		 var form = $('#companyinfo')[0];
	     var data = new FormData(form);
	     $.ajax({
	         type: "POST",
	         enctype: 'multipart/form-data',
	         url: "editProfileCompanyForm1",
	         data: data,
	         processData: false,
	         contentType: false,
	         cache: false,
	         timeout: 600000,
	         success: function (data) {
	        	 $("#tab").tabs();
	        	 //alert("ajaxxxxxxxxxx");
	        	    var selected = $("#tab").tabs("option","active");
	        	   // alert("selected "+ selected);
	        	    $("#tab").tabs("option", "active", selected+1);
	        	   // alert("#tab-"+(selected+1));
	        	    $("#tab-"+(selected+1)).addClass("active in");
	        	    $("#li-tab-"+(selected+1)).addClass("active");
	        	    $("#li-tab-"+(selected)).removeClass("active");
				},
	         error: function (e) {
				}
	     });
	 }
}

</script>



 <!--        end form  -->
</div>
</div>

