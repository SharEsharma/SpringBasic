<script>
function putvalue(){

if($("#check").is(":checked")==true){

var changevalue=document.forms['companyinfo']['headofficeaddress'].value;
$("#currentaddress").val(changevalue);

//fetch country and state---if same address-supriya&mitali
	$("#branch_country").val($("#head_country").val());
	$.post("ajaxCalls?ajaxName=getStates&country="+$("#branch_country").val() , function(data, status){
	//REMOVING '[' ']' FROM THE STRING SO SPLIT CAN BE APPLIED ON THE STRING WITH ONLY COMA'S(,)
	data = data.substring( 1 , data.length-1 );
    var statesArray = data.split(',');
    if($("#branch_state").val()==0){
	var sel = document.getElementById('branch_state');
	for(var i = 0; i < statesArray.length; i++) {
	    var opt = document.createElement('option');
	    opt.innerHTML = statesArray[i];
	    //opt.value = i;
	    opt.value = statesArray[i];
	    sel.appendChild(opt); 
	}
	$("#branch_state").val($("#head_state").val());
}

});
//fetch city---if same address-supriya&mitali
$.post("ajaxCalls?ajaxName=getCities&state="+$("#head_state").val(), function(data, status){
	
	
	//REMOVING '[' ']' FROM THE STRING SO SPLIT CAN BE APPLIED ON THE STRING WITH ONLY COMA'S(,)
	data = data.substring( 1 , data.length-1 );

    var citiesArray = data.split(',');
    if($("#branch_city").val()==0){
	var sel = document.getElementById('branch_city');
	for(var i = 0; i < citiesArray.length; i++) {
	    var opt = document.createElement('option');
	    opt.innerHTML = citiesArray[i];
	    //opt.value = i;
	    opt.value = citiesArray[i];
	    sel.appendChild(opt);
	}
	$("#branch_city").val($("#head_city").val());
    }
    $("#currentaddress").attr('readOnly', true);
    $("#branch_country").attr('disabled', true) ;
    $("#branch_state").attr('disabled', true);
    $("#branch_city").attr('disabled', true);
   //document.getElementById("branch_state").readOnly = true;
    
});
}else{
	$("#currentaddress").val("");
	$("#currentaddress").attr('readOnly', false);
    $("#branch_country").attr('disabled', false);
    $("#branch_state").attr('disabled', false);
    $("#branch_city").attr('disabled', false);
   
}
} 

</script>
