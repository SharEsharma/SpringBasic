<script>
//SubmitHRDetails ajax function and validation of fields
//created by Supriya & Mitali
function SubmitHRDetails(){
	var fn=$("#firstName").val();
	var ln=$("#lastName").val();
	var e=$("#emailId").val();
	var g=$("input[name='gender']:checked").val();
	var d=$("#desc").val();
	var reg =/^[\w\-\.\+]+\@[a-zA-Z0-9\.\-]+\.[a-zA-z0-9]{2,4}$/;
	  if($("#firstName").val()==0){
	    	alert("firstName should not be empty");
	    }
	 else if($("#lastName").val()==0){
	    	alert("lastName should not be empty");
	    }
	 else if($("#emailId").val()==0){
	    	alert("emailId should not be empty");       
	    }
	 else if (reg.test(e) == false) 
     {
		 alert('Invalid Email Address');
     }
	  else if($("#desc").val()==0){
	    	alert("desc should not be empty");
	    } 
	 else{
			 $.ajax({
		         type: "POST",
		         url: "editProfileCompanyForm2",
		         data:{"firstName":fn,"lastName":ln,"emailId":e,"gender":g,"desc":d},
		     	success: function (data) {
		        	 $("#tab").tabs();
		       		 var selected = $("#tab").tabs("option","active");
		        	    if(selected==0){
		        	    	  $("#tab").tabs("option", "active", selected+2); 	  
		   	        	    $("#tab-"+(selected+2)).addClass("active in");
		   	        	    $("#li-tab-"+(selected+2)).addClass("active");
		   	        	    $("#li-tab-"+(selected+1)).removeClass("active");
		        	    }
		        	    else if(selected==1){
		        	    	 $("#tab").tabs("option", "active", selected+1);
			   	        	    $("#tab-"+(selected+1)).addClass("active in");
			   	        	    $("#li-tab-"+(selected+1)).addClass("active");
			   	        	    $("#li-tab-"+(selected)).removeClass("active");
		        	    }
		        	},
		         error: function (e) {
					}
		     });
		 }  
}

</script>



 <!--        end form  -->
</div>
</div>    </div>
