	jQuery(document).ready(function($) {
		$(".scroll").click(function(event){
			event.preventDefault();
			$('html,body').animate({scrollTop:$(this.hash).offset().top},1000);
		});

		 addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false);
			function hideURLbar(){ window.scrollTo(0,3); } 

		 //Coded by Harshad Kamble

		 //FETCH COUNTRIES
			$.post("ajaxCalls?ajaxName=getCountries", function(data, status){

					//REMOVING '[' ']' FROM THE STRING SO SPLIT CAN BE APPLIED ON THE STRING WITH ONLY COMA'S(,)
					data = data.substring( 1 , data.length-1 );

			        var countriesArray = data.split(',');

					$('#head_country').empty();
					var sel1 = document.getElementById('head_country');
					for(var i = 0; i < countriesArray.length; i++) {
					    var opt = document.createElement('option');
					    opt.innerHTML = countriesArray[i];
					    //opt.value = i;
					    opt.value = countriesArray[i];
					    sel1.appendChild(opt);
					}

					$('#branch_country').empty();
					var sel2 = document.getElementById('branch_country');
					for(var i = 0; i < countriesArray.length; i++) {
					    var opt = document.createElement('option');
					    opt.innerHTML = countriesArray[i];
					    //opt.value = i;
					    opt.value = countriesArray[i];
					    sel2.appendChild(opt);
					}
			    });

			//FOR COUNTRIES
		 	//FETCH STATES ON CLICK ON COUNTRY
		 	
		 	$('body').on('change', '#head_country', function() {

				var country = $(this).find("option:selected").text();


				//AJAX CALL TO GET STATES OF THE SELECTED COUNTRY
				$.post("ajaxCalls?ajaxName=getStates&country="+ country , function(data, status){
					console.log(data);

					//REMOVING '[' ']' FROM THE STRING SO SPLIT CAN BE APPLIED ON THE STRING WITH ONLY COMA'S(,)
					data = data.substring( 1 , data.length-1 );

			        var statesArray = data.split(',');

					$('#head_state').empty();
					var sel = document.getElementById('head_state');
					for(var i = 0; i < statesArray.length; i++) {
					    var opt = document.createElement('option');
					    opt.innerHTML = statesArray[i];
					    //opt.value = i;
					    opt.value = statesArray[i];
					    sel.appendChild(opt);
					}
			    });
			});
			$('body').on('change', '#branch_country', function() {
					var country = $(this).find("option:selected").text();
					//AJAX CALL TO GET STATES OF THE SELECTED COUNTRY
					$.post("ajaxCalls?ajaxName=getStates&country="+ country , function(data, status){

						//REMOVING '[' ']' FROM THE STRING SO SPLIT CAN BE APPLIED ON THE STRING WITH ONLY COMA'S(,)
						data = data.substring( 1 , data.length-1 );
			
				        var statesArray = data.split(',');

						$('#branch_state').empty();
						var sel = document.getElementById('branch_state');
						for(var i = 0; i < statesArray.length; i++) {
						    var opt = document.createElement('option');
						    opt.innerHTML = statesArray[i];
						    //opt.value = i;
						    opt.value = statesArray[i];
						    sel.appendChild(opt); 
						}
				    });
				});

			//FOR STATES    
			//FETCH CITIES ON CLICK ON STATE
		    $('body').on('change', '#head_state', function() {

		    	var state = $(this).find("option:selected").text();

		    	$.post("ajaxCalls?ajaxName=getCities&state="+ state , function(data, status){

					//REMOVING '[' ']' FROM THE STRING SO SPLIT CAN BE APPLIED ON THE STRING WITH ONLY COMA'S(,)
					data = data.substring( 1 , data.length-1 );

			        var citiesArray = data.split(',');

			        $('#head_city').empty();
					var sel = document.getElementById('head_city');
					for(var i = 0; i < citiesArray.length; i++) {
					    var opt = document.createElement('option');
					    opt.innerHTML = citiesArray[i];
					    //opt.value = i;
					    opt.value = citiesArray[i];
					    sel.appendChild(opt);
					}
			    });
		    });

		    $('body').on('change', '#branch_state', function() {
			    	var state = $(this).find("option:selected").text();

			    	$.post("ajaxCalls?ajaxName=getCities&state="+ state , function(data, status){
			    		
					
						//REMOVING '[' ']' FROM THE STRING SO SPLIT CAN BE APPLIED ON THE STRING WITH ONLY COMA'S(,)
						data = data.substring( 1 , data.length-1 );

				        var citiesArray = data.split(',');

						$('#branch_city').empty();
						var sel = document.getElementById('branch_city');
						for(var i = 0; i < citiesArray.length; i++) {
						    var opt = document.createElement('option');
						    opt.innerHTML = citiesArray[i];
						    //opt.value = i;
						    opt.value = citiesArray[i];
						    sel.appendChild(opt);
						}
				    });
			    });
	});
	
