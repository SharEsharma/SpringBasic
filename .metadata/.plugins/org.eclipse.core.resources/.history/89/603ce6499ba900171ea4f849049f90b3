<% 

int applicationstatus = 0;

												// declare all id
												int appliedjobid = 0, candidateid = 0, joblistid = 0, gendervalue = 0, candidatescore = 0, exp = 0,
														current_sal = 0, exp_sal = 0;

												// to declare candidate varible
												String firstname = null, country_name = null, state_name = null, city_name = null, lastname = null,
														skillname1 = null, candidatedesc = null, gender = null, qualification = null,
														skilllistid = null;
												String jobtitle = null;

												String jobqullistid = null, qulification_name = null;

												int countryid = 0, stateid = 0, cityid = 0;

												int j = 1;

												//
												int sid = 0;

												// how to set joblist id
												int jobid = 0;

												// set session value in joblistid

												// to find all applied id which is depend on recuirter id & job_list_id
												String sql = null;

												sql = "SELECT `applied_job_id`,`candidate_id`,`job_list_id` FROM `applied_job_list` WHERE `recruiter_id`=1";

												PreparedStatement p = conn.prepareStatement(sql);
												ResultSet rs = p.executeQuery();
												while (rs.next()) {
													appliedjobid = rs.getInt(1);
													candidateid = rs.getInt(2);

													// to fetch candidate details which is depends on candidateid
													String sql1 = "SELECT `candidate_first_name`,`candidate_last_name`,`candidate_desc`,`candidate_gender`,`candidate_qual_list`,`candidate_exp`,`candidate_cur_sal`,`candidate_exp_sal`,`candidate_apt_score`,`candidate_skills_list` FROM `candidate` WHERE `candidate_id`="
															+ candidateid + "";
													PreparedStatement p1 = conn.prepareStatement(sql1);
													ResultSet rs1 = p1.executeQuery();
													while (rs1.next()) {
														firstname = rs1.getString(1);
														lastname = rs1.getString(2);
														candidatedesc = rs1.getString(3);
														gendervalue = rs1.getInt(4);
														qualification = rs1.getString(5);
														exp = rs1.getInt(6);
														current_sal = rs1.getInt(7);
														exp_sal = rs1.getInt(8);
														candidatescore = rs1.getInt(9);
														skilllistid = rs1.getString(10);

													}

													// to fetch country id, state id, city id
													String s = "SELECT `country_id`,`state_id`,`city_id` FROM `candidate` WHERE `candidate_id`="
															+ candidateid + "";
													PreparedStatement pt = conn.prepareStatement(s);
													ResultSet r = pt.executeQuery();
													while (r.next()) {
														countryid = r.getInt(1);
														stateid = r.getInt(2);
														cityid = r.getInt(3);
													}

													// get country name
													String cname = "SELECT `country_name` FROM `country_master` WHERE `id`=" + countryid + "";
													PreparedStatement cname1 = conn.prepareStatement(cname);
													ResultSet r1 = cname1.executeQuery();
													while (r1.next()) {
														country_name = r1.getString(1);
													}

													// get state name
													String sname = "SELECT `state_name` FROM `state_master` WHERE `state_id`=" + stateid + "";
													PreparedStatement sname1 = conn.prepareStatement(sname);
													ResultSet r2 = sname1.executeQuery();
													while (r2.next()) {
														state_name = r2.getString(1);
													}

												// get city name
													
													//Modify by Vikrant
													String ciname = "SELECT `city_name` FROM `city_master` WHERE `city_id`=" + cityid + "";
													PreparedStatement ciname1 = conn.prepareStatement(ciname);
													ResultSet r3 = ciname1.executeQuery();
													while (r3.next()) {
														city_name = r3.getString(1);
													}

													System.out.println("Country id1:" + countryid + "\tState id1:" + stateid + "\tCity id1:" + cityid);

													joblistid = rs.getInt(3);
													String sql4 = "SELECT `job_title`,`job_country_id`,`job_state_id`,`job_city_id`,`job_qual_id_list` FROM `job_list` WHERE `job_id`="
															+ joblistid + "";
													PreparedStatement p4 = conn.prepareStatement(sql4);
													ResultSet rs4 = p4.executeQuery();
													while (rs4.next()) {
														jobtitle = rs4.getString(1);
														jobqullistid = rs4.getString(5);

														// to find particular qualification using jobqulification id
														/* String sql05 = "SELECT `qual_name` FROM `qualification_master` WHERE `qual_id`=" + jobqullistid
																+ ""; */
																
																// here change by shubham sharma
																
															String sql05="select `qual_name` from `qualification_master` where `qual_id`="+qualification+ "";
														PreparedStatement p07 = conn.prepareStatement(sql05);
														ResultSet rs07 = p07.executeQuery();
														while (rs07.next()) {
															qulification_name = rs07.getString(1);
														}

													}

													if (gendervalue == 1) {
														gender = "Male";
													}

													if (gendervalue == 2) {
														gender = "Female";
													}

													String appstatus = "SELECT `application_status` FROM `applied_job_list` WHERE `applied_job_id`="
															+ appliedjobid + "";
													PreparedStatement appStatus = conn.prepareStatement(appstatus);
													ResultSet apps = appStatus.executeQuery();
													while (apps.next()) {
														applicationstatus = apps.getInt(1);
													}
